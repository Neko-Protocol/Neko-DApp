{
  "name": "neko-dapp",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "packageManager": "npm@10.2.3",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "npm run generate:errors && turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\"",
    "generate:errors": "node scripts/generate-error-types.js",
    "check:errors": "node scripts/generate-error-types.js && git diff --exit-code apps/web-app/src/lib/constants/generated/contract-errors.ts",
    "prepare": "husky"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/contracts/*"
  ],
  "dependencies": {
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "tailwindcss": "^4",
    "turbo": "^2.5.8",
    "typescript": "^5"
  },
  "overrides": {
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
